{
  "name": "trustid-near",
  "version": "1.0.0",
  "license": "(MIT AND Apache-2.0)",
  "type": "module",
  "scripts": {
    "build": "near-sdk-js build src/contract.ts build/trustid-near.wasm",
    "build:new": "near-sdk-js build src/NewContract.ts build/trustid-near-new.wasm",
    "build:2": "near-sdk-js build src/MyContract.ts build/trustid-near-mine.wasm",
    "deploy": "near deploy yusufdimari2.testnet build/trustid-near-mine.wasm",
    "test": "$npm_execpath run build && ava -- ./build/trustid-near.wasm",
    "test:2": "$npm_execpath run build:2 && ava -- ./build/trustid-near-mine.wasm"
  },
  "dependencies": {
    "near-sdk-js": "^0.4.0-5"
  },
  "devDependencies": {
    "ava": "^6.1.3",
    "near-workspaces": "^3.5.0",
    "typescript": "^5.6.2"
  },
  "ava": {
    "files": [
      "sandbox-test/*.ava.js"
    ]
  }
}
